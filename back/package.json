{
  "name": "pickforme-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "start": "NODE_ENV=production NODE_PATH=./dist node dist/app.js",
    "build": "node --max-old-space-size=2048 ./node_modules/typescript/bin/tsc && copyfiles -u 1 \"src/feature/analytics/queries/**/*.sql\" dist/",
    "dev": "NODE_PATH=./src ts-node-dev src/app.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "pm2": "pm2 kill && pm2 start ./pm2.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:coverage": "jest --coverage",
    "process-hansiryun": "NODE_ENV=production NODE_PATH=./src ts-node src/scripts/events/hansiryun.ts",
    "process-pickforme-test": "NODE_ENV=production NODE_PATH=./src ts-node src/scripts/events/pickforme_test_user.ts",
    "query-membership-users": "NODE_ENV=production NODE_PATH=./src ts-node src/scripts/events/query_membership_users.ts",
    "manual-etl-pipeline": "NODE_ENV=production NODE_PATH=./src ts-node src/feature/analytics/analytics-manual.ts",
    "test-backup": "NODE_PATH=./src ts-node -e \"import('./src/scheduler/backup').then(m => m.testBackupScheduler())\"",
    "migration:product": "NODE_ENV=production NODE_PATH=./src ts-node src/scripts/migration/product-migration.ts",
    "migration:membership": "NODE_ENV=production NODE_PATH=./src ts-node src/scripts/migration/membership-migration.ts"
  },
